#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55671dcea450 .scope module, "d_flip_flop_test_bench" "d_flip_flop_test_bench" 2 1;
 .timescale 0 0;
v0x55671dcff7a0_0 .var "D", 0 0;
v0x55671dcff860_0 .net "Q", 0 0, v0x55671dcb2ba0_0;  1 drivers
v0x55671dcff930_0 .var "clk", 0 0;
v0x55671dcffa30_0 .var "reset", 0 0;
S_0x55671dcea5e0 .scope module, "DUT" "d_flip_flop" 2 4, 3 1 0, S_0x55671dcea450;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "reset";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "D";
    .port_info 3 /OUTPUT 1 "Q";
v0x55671dcb27b0_0 .net "D", 0 0, v0x55671dcff7a0_0;  1 drivers
v0x55671dcb2ba0_0 .var "Q", 0 0;
v0x55671dcff560_0 .net "clk", 0 0, v0x55671dcff930_0;  1 drivers
v0x55671dcff630_0 .net "reset", 0 0, v0x55671dcffa30_0;  1 drivers
E_0x55671dcb1710 .event posedge, v0x55671dcff560_0;
    .scope S_0x55671dcea5e0;
T_0 ;
    %wait E_0x55671dcb1710;
    %load/vec4 v0x55671dcff630_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55671dcb2ba0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55671dcb27b0_0;
    %assign/vec4 v0x55671dcb2ba0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55671dcea450;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55671dcff930_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x55671dcea450;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x55671dcff930_0;
    %inv;
    %store/vec4 v0x55671dcff930_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55671dcea450;
T_3 ;
    %vpi_call 2 10 "$dumpfile", "d_flip_flop.vcd" {0 0 0};
    %vpi_call 2 11 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55671dcea450 {0 0 0};
    %vpi_call 2 12 "$monitor", $time, " reset = %b, D = %b, Q = %b", v0x55671dcffa30_0, v0x55671dcff7a0_0, v0x55671dcff860_0 {0 0 0};
    %delay 2, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55671dcffa30_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55671dcff7a0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55671dcffa30_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55671dcff7a0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55671dcff7a0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55671dcff7a0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55671dcff7a0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55671dcff7a0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55671dcff7a0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55671dcff7a0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55671dcff7a0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55671dcff7a0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55671dcff7a0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "d_flip_flop_test_bench.v";
    "d_flip_flop.v";
